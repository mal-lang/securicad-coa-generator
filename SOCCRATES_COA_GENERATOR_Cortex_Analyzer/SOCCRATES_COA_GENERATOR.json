{
    "name": "SOCCRATES_Coa_Generator",
    "version": "1.0",
    "author": "KTH",
    "url": "https://github.com/TheHive-Project/Cortex-Analyzers",
    "license": "GPL-V3",
    "description": "Compute COA",
    "dataTypeList": ["other"],
    "baseConfig": "SOCCRATES_COA_GENERATOR",
    "config": {
        "check_tlp": false,
        "service": "coagen",
        "max_tlp": 1
    },
    "command": "SOCCRATES_COA_GENERATOR/soccratesCoaGenerator.py",
    "configurationItems": [
        {
            "name": "url",
            "description": "url of the coagen service",
            "type": "string",
            "multi": false,
            "required": true
        },
        {
            "name": "scad_url",
            "description": "url of the securicad service",
            "type": "string",
            "multi": false,
            "required": true
        },
        {
            "name": "username",
            "description": "username of the securicad service",
            "type": "string",
            "multi": false,
            "required": true
        },
        {
            "name": "password",
            "description": "password of the securicad service",
            "type": "string",
            "multi": false,
            "required": true
        },
        {
            "name": "organization_name",
            "description": "Organization name in the securicad service",
            "type": "string",
            "multi": false,
            "required": true
        },
        {
            "name": "project_name",
            "description": "Project id of the securicad service",
            "type": "string",
            "multi": false,
            "required": true
        },
        {
            "name": "scenario_name",
            "description": "Scenario name in the securicad service, if empty a new one will be created",
            "type": "string",
            "multi": false,
            "required": true
        }


    ]
}
